{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/reportExtension/1.0.0/schema.json",
  "name": "extension",
  "entities": [
    {
      "name": "Agreement Measures",
      "measures": [
        {
          "name": "Number of OneSub Customers",
          "dataType": "Double",
          "expression": "CALCULATE([Number of Customers],'Billing Agreements'[OneSub Flag] = \"OneSub\")",
          "references": {
            "measures": [
              {
                "entity": "Agreement Measures",
                "name": "Number of Customers"
              }
            ]
          }
        },
        {
          "name": "Number of Non-OneSub Customers",
          "dataType": "Double",
          "expression": "CALCULATE([Number of Customers],'Billing Agreements'[OneSub Flag] = \"Not OneSub\")",
          "references": {
            "measures": [
              {
                "entity": "Agreement Measures",
                "name": "Number of Customers"
              }
            ]
          }
        },
        {
          "name": "Number of Orders All Preferences",
          "dataType": "Double",
          "expression": "CALCULATE([Number of Orders], ALL(Preferences))",
          "references": {
            "measures": [
              {
                "entity": "Order Measures",
                "name": "Number of Orders"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "Order Measures",
      "measures": [
        {
          "name": "Diff Share of Orders With Swaps by Preselector",
          "dataType": "Double",
          "expression": "CALCULATE([Share of Orders with Swaps],'Billing Agreements'[Preselector Flag]=\"Preselector\")-CALCULATE([Share of Orders with Swaps],'Billing Agreements'[Preselector Flag]=\"Not Preselector\")",
          "formatString": "0\\ %;-0\\ %;0\\ %",
          "references": {
            "measures": [
              {
                "entity": "Order Measures",
                "name": "Share of Orders with Swaps"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "Date",
      "measures": [
        {
          "name": "Next Menu Week Date",
          "dataType": "Double",
          "expression": "TODAY()+7"
        }
      ]
    }
  ]
}