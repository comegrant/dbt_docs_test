    measure 'Number of Redelivery per 1000 Orders (Diff PY)' = CALCULATE([Number of Redelivery per 1000 Orders], 'Time Intelligence'[Time Calculation]="Diff PY") 
		formatString: #,0
        displayFolder: Redeliveries\Per 1000 Orders

