    /// Need to be filtered on year if its wanted to see numbers for a specific year.
    measure 'Number of Redelivery per 1000 Orders (Current Month)' = CALCULATE([Number of Redelivery per 1000 Orders], 'Time Intelligence'[Time Calculation]="Current Month") 
		formatString: 0.00%;-0.00%;0.00%
        displayFolder: Redeliveries\Per 1000 Orders

